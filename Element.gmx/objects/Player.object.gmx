                    y -= (distance_to_object(instance_nearest(x, y - jumpHeight, notPlayer)) - 1);
                    jumpHeight = 0;
                }
            }
            
        }
    } else {
        if (place_free(x,y + fallMult + fall)) {
            if (fall &lt; fallMax) {
                fall += fallMult;
            }
            y += fall;
        }
        else {
            fall = 0;
        }
    }
}
