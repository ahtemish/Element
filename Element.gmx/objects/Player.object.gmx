            <string>///  Do gravity

if(!ladder) {
    if(jump) {
        if(jumpHeight == 0) {
            jump = false;
        } else {
            if(place_free(x, y - jumpHeight)) {
                y -= jumpHeight;
                jumpHeight--;
            } else {
                if(place_meeting(x, y - jumpHeight, trapDoor)) {
                    y -= jumpHeight;
                    jumpHeight--;
                    instance_deactivate_object(trapDoor);
                    instance_activate_object(falseTrapDoor);
