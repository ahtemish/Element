        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Attack

if(!dead) {
    if(!punch) {
        num = 0;
        punch = true;
        
        if(airAbility) {
            alarm[3] = room_speed * (4/30); 
            if(left) {
                sprite_index = punchLeftAir_spr;
                global.enemyHitId[num] = collision_rectangle(x - (sprite_width / 2), y, x, y + sprite_height, enemy, false, true);
                global.enemyHitType[num] = "air";        
            } else {
                sprite_index = punchRightAir_spr;
                global.enemyHitId[num] = collision_rectangle(x + sprite_width, y, x + (sprite_width * 1.5), y + sprite_height, enemy, false, true);
                global.enemyHitType[num] = "air";
            }
        } else if(earthAbility) {
            alarm[3] = room_speed * (12/30);
            if(left) {
                sprite_index = punchLeftEarth_spr;
                for (j = 0; j &lt; instance_number(enemy); j++) {
                    possibleHit = instance_find(enemy, j);
                    if(possibleHit == collision_rectangle(x - sprite_width, y, x, y + sprite_height, possibleHit, false, true)) {
                        global.enemyHitId[num] = possibleHit;
                        global.enemyHitType[num] = "earth";
                        num++;
                    }
                }
            } else {
                sprite_index = punchRightEarth_spr;
                for (j = 0; j &lt; instance_number(enemy); j++) {
                    possibleHit = instance_find(enemy, j);
                    if(possibleHit == collision_rectangle(x + sprite_width, y, x + (sprite_width * 2), y + sprite_height, possibleHit, false, true)) {
                        global.enemyHitId[num] = possibleHit;
                        global.enemyHitType[num] = "earth";
                        num++;
                    }
                }
            }
        } else if(waterAbility) {
            alarm[3] = room_speed * (8/30);
            drawWhip = true;
            if(left) {
                sprite_index = punchLeftWater_spr;
                for (j = 0; j &lt; instance_number(enemy); j++) {
                    possibleHit = instance_find(enemy, j);
                    if(possibleHit == collision_rectangle(x - waterwhipLeft_spr.sprite_width, y, x, y + sprite_height, possibleHit, false, true)) {
                        global.enemyHitId[num] = possibleHit;
                        global.enemyHitType[num] = "water";
                        num++;
                    }
                }
            } else {
                sprite_index = punchRightWater_spr;
                for (j = 0; j &lt; instance_number(enemy); j++) {
                    possibleHit = instance_find(enemy, j);
                    if(possibleHit == collision_rectangle(x + sprite_width, y, x + sprite_width + waterwhipRight_spr.sprite_width, y + sprite_height, possibleHit, false, true)) {
                        global.enemyHitId[num] = possibleHit;
                        global.enemyHitType[num] = "water";
                        num++;
                    }
                }
            }
        } else {
            alarm[3] = room_speed * (8/30);
            if(left) {
                sprite_index = punchLeftFire_spr;
                plFireball = instance_create(x - 5, y + 8, fireball);
            } else {
                sprite_index = punchRightFire_spr;
                plFireball = instance_create(x + 5, y + 8, fireball);
            }
            plFireball.firedBy = Player;
        }
    }
}
</string>
